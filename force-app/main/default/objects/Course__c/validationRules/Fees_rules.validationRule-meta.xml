<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Fees_rules</fullName>
    <active>true</active>
    <errorConditionFormula>OR( ISBLANK(Fees__c ) ,Fees__c &lt; 5000, Fees__c &gt; 500000 )</errorConditionFormula>
    <errorDisplayField>CourseTitle__c</errorDisplayField>
    <errorMessage>Fees is also be requried and must be in range 5000 to 500000.</errorMessage>
</ValidationRule>
