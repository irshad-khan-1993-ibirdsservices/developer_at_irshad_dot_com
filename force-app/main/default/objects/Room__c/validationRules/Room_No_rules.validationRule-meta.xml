<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Room_No_rules</fullName>
    <active>true</active>
    <errorConditionFormula>NOT( REGEX( Room_No__c , &quot;^RM_.{0,5}$&quot;) )</errorConditionFormula>
    <errorDisplayField>Room_No__c</errorDisplayField>
    <errorMessage>First 3 letters of room must &apos;RM_&apos; and length must not greater than 8 characters.</errorMessage>
</ValidationRule>
