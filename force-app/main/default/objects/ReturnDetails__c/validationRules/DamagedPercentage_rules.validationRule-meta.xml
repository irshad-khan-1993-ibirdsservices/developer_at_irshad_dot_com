<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>DamagedPercentage_rules</fullName>
    <active>true</active>
    <errorConditionFormula>AND( ISPICKVAL( Status__c , &quot;Damaged&quot;) , ISBLANK( DamagedPercentage__c ) )</errorConditionFormula>
    <errorDisplayField>DamagedPercentage__c</errorDisplayField>
    <errorMessage>If Status is &quot;Damaged&quot;, then DamagedPercentage is requried.</errorMessage>
</ValidationRule>
