<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>UnitPrice_rules</fullName>
    <active>true</active>
    <errorConditionFormula>NOT((UnitPrice__c   &gt;= 45000  &amp;&amp;  UnitPrice__c   &lt;=  100000))</errorConditionFormula>
    <errorDisplayField>UnitPrice__c</errorDisplayField>
    <errorMessage>UnitPrice must be between 45000 to 100000</errorMessage>
</ValidationRule>
