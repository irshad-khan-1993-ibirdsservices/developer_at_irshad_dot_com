<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Stock_rules</fullName>
    <active>true</active>
    <errorConditionFormula>NOT( AND( Stock__c  &gt;= 10, Stock__c  &lt;= 80) )</errorConditionFormula>
    <errorDisplayField>Stock__c</errorDisplayField>
    <errorMessage>Stock must be in range  of 10 to 80.</errorMessage>
</ValidationRule>
